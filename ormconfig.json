{
   "type": "mysql",
   "host": "localhost",
   "port": 3306,
   "username": "*your username*",
   "password": "*your password*",
   "database": "DataBase Name",
   "synchronize": false,//if it is true,it will create new tables.keep it "false" for exist old tables 
   "logging": false,
   "entities": [
      "src/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
